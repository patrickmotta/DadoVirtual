{
  "expo": {
    "name": "Dado Virtual",
    "slug": "DadoVirtual",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-dice.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-dice.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.patrickmotta100.DadoVirtual"
    },
    "web": {
      "favicon": "./assets/icon-dice.png"
    }
  }
}
